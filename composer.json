{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for Zend Framework zend-mvc applications",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf3"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
  	  "php": "^7.3",
  	  "ext-gd": "*",
  	  "ext-intl": "*",
      "laminas/laminas-authentication": "^2.5",
      "laminas/laminas-cache": "^2.7",
      "laminas/laminas-captcha": "^2.6",
  	  "laminas/laminas-component-installer": "^1.0 || ^2.1",
      "laminas/laminas-crypt": "^3.1",
      "laminas/laminas-db": "^2.10",
      "laminas/laminas-developer-tools":"^1.0",
      "laminas/laminas-development-mode": "^3.2",
      "laminas/laminas-form": "^2.14",
      "laminas/laminas-i18n": "^2.6",
      "laminas/laminas-mail": "^2.10",
      "laminas/laminas-math": "^3.0",
      "laminas/laminas-mime": "^2.7",
      "laminas/laminas-modulemanager": "2.8.4",
  	  "laminas/laminas-mvc": "^3.3",
  	  "laminas/laminas-mvc-form": "^1.0",
  	  "laminas/laminas-mvc-plugins": "^1.0.1",
      "laminas/laminas-navigation": "^2.9",
      "laminas/laminas-paginator": "^2.8",
      "laminas/laminas-permissions-rbac": "^2.5",
  	  "laminas/laminas-serializer": "^2.8",
  	  "laminas/laminas-session": "^2.0",
  	  "laminas/laminas-test": "^3 || ^4",
  	  "doctrine/doctrine-orm-module": "^4",
  	  "doctrine/migrations": "^3",
  	  "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "User\\": "module/User/src/",
            "Quote\\": "module/Quote/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/",
            "UserTest\\": "module/User/test/",
            "QuoteTest\\": "module/Quote/test/"
        }
    },
    "extra": [],
    "scripts": {
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
        "test": "phpunit"
    }
}
